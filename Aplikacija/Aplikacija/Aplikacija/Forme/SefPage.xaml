<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Aplikacija.Forme.SefPage"
             BackgroundColor="Black">
    <ScrollView VerticalScrollBarVisibility="Always">
        <StackLayout Margin="0,30,0,0">
            <Button x:Name="PregledPozicija" Text="Pregledaj pozicije" Background="#ffc100" CornerRadius="40" TextColor="White" FontFamily="Helvetica" FontAttributes="Bold" FontSize="19" Clicked="PregledPozicija_Clicked" />
            <Button x:Name="PregledVozaca" Text="Pregledaj vozace" Background="#ffc100" CornerRadius="40" TextColor="White" FontFamily="Helvetica" FontAttributes="Bold" FontSize="19" Clicked="PregledVozaca_Clicked" />
            <Button x:Name="PregledNegativnih" Text="Pregledaj negativne ocene" Background="#ffc100" CornerRadius="40" TextColor="White" FontFamily="Helvetica" FontAttributes="Bold" FontSize="19" Clicked="PregledNegativnih_Clicked" />

        </StackLayout>
    </ScrollView>
</ContentPage>