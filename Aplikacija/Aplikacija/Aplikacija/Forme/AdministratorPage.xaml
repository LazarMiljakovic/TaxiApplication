<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Aplikacija.Forme.AdministratorPage"
             BackgroundColor="Black">
    <ScrollView VerticalScrollBarVisibility="Always">
        <StackLayout Margin="0,30,0,0">
            <Button x:Name="NapraviAdmin" Text="Napravi nalog Administratora" Background="#ffc100" CornerRadius="40" TextColor="White" FontFamily="Helvetica" FontAttributes="Bold" FontSize="19" Clicked="NapraviAdmin_Clicked" />
            <Button x:Name="NapraviSef" Text="Napravi nalog Sefa" Background="#ffc100" CornerRadius="40" TextColor="White" FontFamily="Helvetica" FontAttributes="Bold" FontSize="19" Clicked="NapraviSef_Clicked" />
            <Button x:Name="NapraviVozac" Text="Napravi nalog Vozaca" Background="#ffc100" CornerRadius="40" TextColor="White" FontFamily="Helvetica" FontAttributes="Bold" FontSize="19" Clicked="NapraviVozac_Clicked" />
            <Button x:Name="Obrisi" Text="Obrisi nalog" Background="#ffc100" CornerRadius="40" TextColor="White" FontFamily="Helvetica" FontAttributes="Bold" FontSize="19" Clicked="Obrisi_Clicked" />
            
        </StackLayout>
    </ScrollView>
</ContentPage>